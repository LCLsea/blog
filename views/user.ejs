<% include header %>
<% posts.forEach(function (post, index) { %>
<div class="essay">
  <p><h2 style="border-bottom: 1px solid #CCCCCC;">
    <a href="/u/<%= post.name %>/<%= post.time.day %>/<%= post.title %>">
    <%= post.title %></a></h2></p>
  <p class="info">
    作者：<a href="/u/<%= post.name %>"><%= post.name %></a> |
    日期：<%= post.time.minute %>
  </p>
  <p><%- post.post %></p>
</div>
<% }) %>
<% include footer %>
